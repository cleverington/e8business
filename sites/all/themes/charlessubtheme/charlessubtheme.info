; Initial custom CSS included in Omega Base
;
; global.css will mostly contain block-specific CSS changes/declarations
; Theme CSS can be found in custom CSS below
;
; Those familiar with Zen Subtheming would recognize the
; characters as:
;    stylesheets[all][]  = style.css
; See http://drupal.org/node/225125 for more information on
;    subtheming.
;
;

css[global.css][name] = 'Custom global styles'
css[global.css][description] = 'This file holds all the globally active custom CSS of your theme.  global.css will be best used with declared additional CSS tags within the wrapper/zone/region declarations.'
css[global.css][options][weight] = '10'

;
; Additional custom CSS as copied, then editied from Omega Base
;
; Custom CSS will generally be used over global.css for clarity
;

css[charlessubtheme-branding.css][name] = 'Custom branding styles'
css[charlessubtheme-branding.css][description] = 'This file holds all the globally active custom CSS for the Branding zone/region of your theme.'
css[charlessubtheme-branding.css][options][weight] = '10'

css[charlessubtheme-forms.css][name] = 'Custom forms styles'
css[charlessubtheme-forms.css][description] = 'This file holds all the globally active custom CSS for the forms zone/region of your theme.'
css[charlessubtheme-forms.css][options][weight] = '10'

css[charlessubtheme-menu.css][name] = 'Custom menu styles'
css[charlessubtheme-menu.css][description] = 'This file holds all the globally active custom CSS for the menu zone/region of your theme.'
css[charlessubtheme-menu.css][options][weight] = '8'

css[charlessubtheme-text.css][name] = 'Custom text styles'
css[charlessubtheme-text.css][description] = 'This file holds all the globally active custom CSS for the text zone/region of your theme.'
css[charlessubtheme-text.css][options][weight] = '8'

css[charlessubtheme-visuals.css][name] = 'Custom visuals styles'
